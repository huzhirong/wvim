	}
# 排行榜
snippet ul-rate-list
	.rate-list li{
		border-bottom: 1px dashed #D3D3D3;
		padding: 10px 0;
		text-align:right;
	}
	.rate-list li.last,.rate-list li:last-child {
		border-bottom:none;
	}
	.rate-list i{
		background-color:#E5E5E5;
		border-radius:3px;
		display:inline;
		float:left;
		font-size:12px;
		height:18px;
		margin-right:6px;
		overflow:hidden;
		text-align:center;
		width:16px;
	}
	.rate-list .title-link{
		display:inline;
		float:left;
		-o-text-overflow:ellipsis;
		overflow: hidden;
		text-align:left;
		text-overflow:ellipsis;
		white-space:nowrap;
		width:70px;
	}
# 用户头像列表	
snippet ul-user-avatar
	.user-avatar{
		margin-right:-10px;
	}
	.user-avatar li,.user-avatar a strong{
		width:56px;
	}
	.user-avatar li,.user-avatar li a{
		display:inline;
		float:left;
	}
	.user-avatar li{
		margin:10px 10px 0 0;
	}
	.user-avatar img{
		background-color:#FFF;
		border:1px solid #CCC;
		border-radius:3px;
		height:50px;
		padding:2px;
		vertical-align:middle;
		width:50px;
	}
	.user-avatar a strong{
		display:block;
		-o-text-overflow: ellipsis;
		text-overflow: ellipsis;		  
		overflow: hidden;
		white-space: nowrap;
		height: 20px;
		line-height:20px;
		font-weight:normal;
		text-align:center;
	}
