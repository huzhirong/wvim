# 无缝滚动
snippet carousel
	.carousel{
		position:relative;
		*zoom:1;
	}
	.carousel .view-content{
		height:156px;
		margin:0 44px;
		overflow:hidden;
		width:860px; 
	}
	.carousel .view-content li{
		display:inline;
		float:left;
		margin-right:15px; 
		text-align:center;   
		width:160px; 
	}
	.carousel li img{
		border:1px solid #ccc;
		height:116px;
		vertical-align:bottom;
		padding:4px;
		width:150px;
	}
	.carousel li strong{
		display:block;
		height:30px;
		line-height:30px;
		-o-text-overflow:ellipsis;
		overflow:hidden;
		text-overflow:ellipsis;
		white-space:nowrap;
		width:160px; 
	}
	.carousel .prev-btn,.carousel .next-btn{
		background:url(images/cont-btn-bg.gif) no-repeat;
		cursor:pointer;
		font-size:0;
		height:80px;
		line-height:0;
		overflow:hidden;
		position:absolute;
		top:29px; 
		width:30px; 
	}
	.carousel .prev-btn{
		left:10px; 
	}
	.carousel .next-btn{
		background-position:right top;
		right:10px;
	}
# 图片滚动
snippet imgslide
	.imgslide{
		position:relative;
	}
	.imgslide .slide-view{
		height:300px;
		overflow:hidden;
		width:550px; 
	}
	.imgslide .slide-view ul{
		width:2200px; 
	}
	.imgslide .slide-control{
		background-color:rgba(0,0,0,.5);
		bottom:0;
		filter:progid:DXImageTransform.Microsoft.gradient(enabled='true',startColorstr='#7F000000', endColorstr='#7F000000');
		height:36px;
		left:0;
		line-height:36px;
		position:absolute;
		width:550px;
	}
	.imgslide .slide-title a{
		color:#fff;
		display:inline-block;
		*display:inline;
		*zoom:1;
		overflow: hidden;
		-o-text-overflow:ellipsis;
		padding-left:10px;
		text-overflow:ellipsis;
		white-space:nowrap;
		width:420px; 
	}
	.imgslide .slide-bullet{
		bottom:10px;
		right:10px;
		position:absolute;
	}
	.imgslide .slide-bullet a{
		background:url() no-repeat;
		cursor:pointer;
		display:inline;
		float:left;
		height:15px;
		margin-right:5px;
		overflow:hidden;
		text-indent:-9999px;
		width:13px;
	}
	.imgslide .slide-bullet a.active, .imgslide .slide-bullet a:hover {
		background-position:0 -15px;
	}
	.imgslide .prev-btn, .imgslide .next-btn{
		background:url() no-repeat;
		cursor:pointer;
		height:36px; 
		position:absolute;
		top:132px; 
		width:32px;
	}
	.imgslide .prev-btn{
		left:0;
	}
	.imgslide .next-btn{
		background-position:0 -36px;
		right:0;
	}
